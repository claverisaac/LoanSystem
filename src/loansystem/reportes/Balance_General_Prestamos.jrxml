	<queryString>
		<![CDATA[select
m.idmoneda,
m.nombre, m.simbolo,
ifnull(sum(p.montoOriginal),0) as totalPrestado,
(select ifnull(sum(a.montoAbonado),0)  from  abono  as a where a.idPrestamo = p.idPrestamo) as totalRecaudado,
(ifnull(sum(p.montoOriginal),0) - (select ifnull(sum(a.montoAbonado),0) from  abono  as a where a.idPrestamo = p.idPrestamo)) as Pendiente
from prestamo as p
inner join moneda as m on m.idMoneda = p.idMoneda
where p.estado = 1
group by m.idMoneda;]]>
	</queryString>
